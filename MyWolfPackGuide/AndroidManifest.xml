    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:name="com.example1.mywolfpackguide.CategoryList"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example1.mywolfpackguide.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example1.mywolfpackguide.SignUpActivity"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="com.example1.mywolfpackguide.CategoryActivity"
            android:label="@string/title_activity_category" >
        </activity>
        <activity
            android:name="com.example1.mywolfpackguide.AdminHomePage"
            android:label="@string/title_activity_admin_home_page" >
        </activity>
        <activity
            android:name="com.example1.mywolfpackguide.UserHomePage"
            android:label="@string/title_activity_user_home_page" >
        </activity>
    </application>

</manifest>
